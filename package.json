{
  "name": "foundationpressng",
  "title": "FoundationPressNG",
  "version": "3.0.0",
  "description": "FoundationPressNG is a WordPress starter theme based on Foundation 6",
  "homepage": "https://phantomoon.com",
  "license": "MIT",
  "main": "gulpfile.babel.js",
  "scripts": {
    "start": "gulp",
    "dev": "gulp build --dev",
    "build": "gulp build --production",
    "package": "gulp package --production",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format-test:prettier": "node_modules/.bin/prettier --config .prettierrc --write \"test/*.{css,js}\" ",
    "lint:css": "stylelint --fix src/assets/scss/**/*.scss",
    "phpcs": "gulp phpcs",
    "phpcbf": "gulp phpcbf"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.css*": [
      "stylelint --fix"
    ]
  },
  "keywords": [
    "FoundationPressNG",
    "WordPress",
    "starter theme",
    "Foundation",
    "semantic",
    "mobile first",
    "customizable",
    "responsive"
  ],
  "dependencies": {
    "foundation-sites": "6.6.3",
    "jquery": "~3.5.1",
    "motion-ui": "^2.0.3",
    "what-input": "^5.2.9"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@wordpress/eslint-plugin": "^6.0.0",
    "ansi-colors": "^4.1.1",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.26.7",
    "dateformat": "^3.0.3",
    "eslint": "^7.1.0",
    "fancy-log": "^1.3.3",
    "git-cz": "^4.7.1",
    "gulp": "^4.0",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-babel": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-cli": "^2.2.1",
    "gulp-concat": "^2.6.1",
    "gulp-extname": "^0.2.2",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-load-plugins": "^2.0.3",
    "gulp-phpcbf": "^0.1.2",
    "gulp-phpcs": "^3.1.0",
    "gulp-rev": "^9.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-stylelint": "^13.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-zip": "^5.0.1",
    "husky": "^4.2.5",
    "js-yaml": "^3.13.1",
    "lint-staged": "^10.2.6",
    "panini": "^1.6.3",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "style-sherpa": "^1.0.2",
    "stylelint": "^13.5.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recess-order": "^2.1.0",
    "stylelint-config-standard-scss": "^1.1.0",
    "stylelint-config-wordpress": "^16.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-prettier": "^1.1.2",
    "uncss": "^0.17.3",
    "vinyl-named": "^1.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-stream": "^5.2.1",
    "yargs": "^15.3.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/annrie/FoundationPressNG.git"
  },
  "bugs": {
    "url": "https://github.com/annrie/FoundationPressNG/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  }
}
