{
  "name": "foundationpressng",
  "title": "FoundationPressNG",
  "version": "3.0.1",
  "description": "FoundationPressNG is a WordPress starter theme based on Foundation 6",
  "homepage": "https://phantomoon.com",
  "license": "MIT",
  "main": "gulpfile.babel.js",
  "scripts": {
    "start": "gulp",
    "dev": "gulp build --dev",
    "build": "gulp build --production",
    "package": "gulp package --production",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format-test:prettier": "node_modules/.bin/prettier --config .prettierrc --write \"test/*.{css,js}\" ",
    "lint:css": "stylelint --fix src/assets/scss/**/*.scss",
    "phpcs": "gulp phpcs",
    "phpcbf": "gulp phpcbf",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.css*": [
      "stylelint --fix"
    ]
  },
  "keywords": [
    "FoundationPressNG",
    "WordPress",
    "starter theme",
    "Foundation",
    "semantic",
    "mobile first",
    "customizable",
    "responsive"
  ],
  "dependencies": {
    "cz-customizable": "^6.3.0",
    "foundation-sites": "^6.7.4",
    "jquery": ">=3.6.0",
    "lodash.template": "^4.5.0",
    "motion-ui": "git+https://github.com/annrie/motion-ui.git",
    "what-input": ">=5.2.10"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/register": "^7.16.0",
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^12.1.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@wordpress/block-editor": "^8.0.3",
    "@wordpress/browserslist-config": "^4.1.0",
    "@wordpress/components": "^14.2.0",
    "@wordpress/eslint-plugin": "^9.2.0",
    "ansi-colors": "^4.1.1",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.27.7",
    "dateformat": "^4.5.1",
    "eslint": "^7.32.0",
    "fancy-log": "^1.3.3",
    "gulp": "^4.0",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-babel": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-cli": "^2.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-extname": "^0.2.2",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-load-plugins": "^2.0.7",
    "gulp-phpcbf": "^0.1.2",
    "gulp-phpcs": "^3.1.0",
    "gulp-rev": "^9.0.0",
    "gulp-sass": "^5.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-stylelint": "^13.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-zip": "^5.1.0",
    "husky": "^7.0.4",
    "js-yaml": "^4.1.0",
    "lint-staged": "^12.0.2",
    "panini": "^1.7.2",
    "postcss": "^8.3.11",
    "postcss-custom-properties": "^12.0.0",
    "prettier": "npm:wp-prettier@latest",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reakit-utils": "^0.15.1",
    "rimraf": "^3.0.2",
    "sass": "^1.38.0",
    "sass-loader": "12.3.0",
    "style-sherpa": "^1.0.2",
    "stylelint": "^13.13.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-property-sort-order-smacss": "^7.1.0",
    "stylelint-config-recess-order": "^2.6.0",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^3.20.1",
    "typescript": "^4.4.2",
    "uncss": "^0.17.3",
    "vinyl-named": "^1.1.0",
    "webpack": "^5.64.1",
    "webpack-cli": "^4.9.1",
    "webpack-stream": "^7.0.0",
    "yargs": "^16.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/annrie/FoundationPressNG.git"
  },
  "bugs": {
    "url": "https://github.com/annrie/FoundationPressNG/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  }
}
