{
  "name": "foundationpressng",
  "title": "FoundationPressNG",
  "version": "3.0.2",
  "description": "FoundationPressNG is a WordPress starter theme based on Foundation 6",
  "homepage": "https://phantomoon.com",
  "license": "MIT",
  "main": "gulpfile.babel.js",
  "scripts": {
    "start": "gulp",
    "dev": "gulp build --dev",
    "build": "gulp build --production",
    "package": "gulp package --production",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format-test:prettier": "node_modules/.bin/prettier --config .prettierrc --write \"test/*.{css,js}\" ",
    "lint:css": "stylelint --fix src/assets/scss/**/*.scss",
    "phpcs": "gulp phpcs",
    "phpcbf": "gulp phpcbf",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.css*": [
      "stylelint --fix"
    ]
  },
  "keywords": [
    "FoundationPressNG",
    "WordPress",
    "starter theme",
    "Foundation",
    "semantic",
    "mobile first",
    "customizable",
    "responsive"
  ],
  "dependencies": {
    "@wordpress/block-editor": "^10.5.0",
    "@wordpress/components": "^22.1.0",
    "cz-customizable": "^6.3.0",
    "date-and-time": "^2.4.1",
    "dateformat": "^5.0.3",
    "foundation-sites": "^6.7.5",
    "jquery": ">=3.6.0",
    "lodash.template": "^4.5.0",
    "motion-ui": "latest",
    "what-input": ">=5.2.10",
    "yargs": "^17.1.1-candidate.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.1",
    "@babel/register": "^7.18.9",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@wordpress/browserslist-config": "^5.5.0",
    "@wordpress/eslint-plugin": "^13.6.0",
    "ansi-colors": "^4.1.1",
    "babel-loader": "^8.2.5",
    "browser-sync": "^2.27.10",
    "clean-webpack-plugin": "^4.0.0",
    "eslint": "^8.20.0",
    "eslint-config-wordpress": "^2.0.0",
    "fancy-log": "^1.3.3",
    "gulp": "^4.0.2",
    "gulp-add-src": "^1.0.0",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-cache-bust": "^1.4.1",
    "gulp-check-deps": "^1.4.1",
    "gulp-clean-css": "^4.3.0",
    "gulp-cli": "^2.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-dart-sass": "^1.0.2",
    "gulp-eslint": "^6.0.0",
    "gulp-extname": "^0.2.2",
    "gulp-filter": "^7.0.0",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-jshint": "^2.1.0",
    "gulp-load-plugins": "^2.0.8",
    "gulp-phpcbf": "^0.1.2",
    "gulp-phpcs": "^3.1.0",
    "gulp-rev": "^9.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sass-lint": "^1.4.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-zip": "^5.1.0",
    "husky": "^8.0.1",
    "js-yaml": "^4.1.0",
    "lint-staged": "^13.0.3",
    "panini": "^1.7.2",
    "postcss": "^8.4.14",
    "postcss-custom-properties": "^12.1.8",
    "postcss-scss": "^4.0.5",
    "prettier": "npm:wp-prettier@latest",
    "prettyjson": "^1.2.5",
    "require-dir": "^1.2.0",
    "requirejs": "^2.3.6",
    "rimraf": "^3.0.2",
    "sass": "^1.54.0",
    "sass-loader": "13.2.0",
    "style-sherpa": "^1.0.2",
    "stylelint": "^14.14.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-property-sort-order-smacss": "^9.0.0",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.3.0",
    "util-deprecate": "^1.0.2",
    "vinyl-named": "^1.1.0",
    "webpack": "^5.73.0",
    "webpack-cli": "^5.0.0",
    "webpack-stream": "^7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/annrie/FoundationPressNG.git"
  },
  "bugs": {
    "url": "https://github.com/annrie/FoundationPressNG/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  }
}
