{
  "name": "phantomoon",
  "title": "FoundationPressNG for Phantomoon",
  "version": "4.0.1",
  "description": "FoundationPressNG is a WordPress starter theme based on Foundation 6",
  "homepage": "https://phantomoon.com",
  "license": "MIT",
  "main": "gulpfile.babel.js",
  "scripts": {
    "start": "gulp --watch",
    "dev": "gulp build --dev",
    "build": "gulp build --production",
    "package": "gulp package --production",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format-test:prettier": "node_modules/.bin/prettier --config .prettierrc --write \"test/*.{css,js}\" ",
    "lint:css": "stylelint --fix src/assets/scss/**/*.scss",
    "phpcs": "gulp phpcs",
    "phpcbf": "gulp phpcbf",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "jquery": ">=3.6.0",
    "motion-ui": "latest",
    "what-input": ">=5.2.10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.css*": [
      "stylelint --fix"
    ]
  },
  "keywords": [
    "phantomoon",
    "FoundationPressNG",
    "WordPress",
    "starter theme",
    "Foundation",
    "semantic",
    "mobile first",
    "customizable",
    "responsive"
  ],
  "dependencies": {
    "caniuse-lite": "^1.0.30001522",
    "date-and-time": "^3.0.2",
    "foundation-sites": "^6.8.1",
    "joi": "^17.9.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/register": "^7.22.5",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "ansi-colors": "^4.1.3",
    "autoprefixer": "^10.4.15",
    "babel-loader": "^9.1.3",
    "browser-sync": "^2.29.3",
    "browserstack-runner": "^0.9.4",
    "chai": "^4.3.7",
    "chai-jquery": "^2.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "clipboard": "^2.0.11",
    "corejs-typeahead": "^1.3.3",
    "cz-conventional-changelog": "^3.3.0",
    "doiuse": "^6.0.2",
    "eslint": "^8.47.0",
    "eslint-config-wordpress": "^2.0.0",
    "fancy-log": "^2.0.0",
    "foundation-docs": "github:foundation/foundation-docs",
    "gulp": "^4.0.2",
    "gulp-add-src": "^1.0.0",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-cache-bust": "^1.4.1",
    "gulp-check-deps": "^1.4.1",
    "gulp-clean-css": "^4.3.0",
    "gulp-cli": "^2.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-dart-sass": "^1.1.0",
    "gulp-debug": "^4.0.0",
    "gulp-eslint": "^6.0.0",
    "gulp-extname": "^0.2.2",
    "gulp-filter": "^8.0.0",
    "gulp-if": "^3.0.0",
    "gulp-jshint": "^2.1.0",
    "gulp-load-plugins": "^2.0.8",
    "gulp-newer": "^1.4.0",
    "gulp-notify": "^4.0.0",
    "gulp-phpcbf": "^0.1.2",
    "gulp-phpcs": "^3.1.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.4",
    "gulp-sass": "^5.1.0",
    "gulp-sass-lint": "^1.4.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-zip": "^5.1.0",
    "gulp4-run-sequence": "^1.0.2",
    "husky": "^8.0.3",
    "is-empty-object": "^1.1.1",
    "jquery": ">=3.7.0",
    "js-yaml": "^4.1.0",
    "kleur": "^4.1.5",
    "lint-staged": "^14.0.1",
    "mocha": "^10.2.0",
    "mocha-headless-chrome": "^4.0.0",
    "motion-ui": "^2.0.5",
    "octophant": "latest",
    "panini": "^1.7.2",
    "patch-package": "^8.0.0",
    "postcss": "^8.4.28",
    "postcss-custom-properties": "^13.3.0",
    "postcss-scss": "^4.0.7",
    "prettier": "npm:wp-prettier@latest",
    "prettyjson": "^1.2.5",
    "require-dir": "^1.2.0",
    "requirejs": "^2.3.6",
    "rimraf": "^3.0.2",
    "rollup": "^3.28.1",
    "rollup-plugin-babel": "^4.3.0",
    "sass": "^1.66.1",
    "sass-loader": "13.3.2",
    "sass-true": "^7.0.0",
    "sassy-lists": "^3.0.1",
    "sinon": "^15.2.0",
    "strip-indent": "^4.0.0",
    "style-sherpa": "^1.0.2",
    "stylelint": "^15.10.3",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-property-sort-order-smacss": "^9.1.0",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0",
    "stylelint-prettier": "^4.0.2",
    "stylelint-scss": "^5.1.0",
    "supercollider": "latest",
    "undertaker-forward-reference": "^2.0.0",
    "util-deprecate": "^1.0.2",
    "vinyl": "^3.0.0",
    "vinyl-named": "^1.1.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-stream": "^7.0.0",
    "what-input": ">=5.2.12"
  },
  "resolutions": {
    "glob-parent": ">=5.1.2",
    "minimist": ">=1.2.3",
    "merge": ">=2.1.1",
    "shelljs": ">=0.8.5",
    "nth-check": ">=2.0.1",
    "lodash.template": ">=4.5.0"
  },
  "engines": {
    "node": ">=16.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/annrie/FoundationPressNG.git"
  },
  "bugs": {
    "url": "https://github.com/annrie/FoundationPressNG/issues"
  },
  "jspm": {
    "main": "dist/js/foundation.esm.js",
    "format": "esm"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "@babel/preset-env"
          ]
        }
      ]
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  }
}
